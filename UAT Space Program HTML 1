                <script>
                    // Variable Definition
                    var currTime = 10;
                    // HTML Element
                    document.getElementById("DisplayCountdown").innerHTML = currTime;
                    // Decrement by one
                    currTime--;

                    document.getElementById("DisplayCountdown").innerHTML = currTime;
                    
                    currTime--;

                    document.getElementById("DisplayCountdown").innerHTML = currTime;
                    
                    currTime--;

                    document.getElementById("DisplayCountdown").innerHTML = currTime;
                    
                    currTime--;

                    document.getElementById("DisplayCountdown").innerHTML = currTime;
                    
                    currTime--;

                    document.getElementById("DisplayCountdown").innerHTML = currTime;
                    
                    currTime--;

                    document.getElementById("DisplayCountdown").innerHTML = currTime;
                    
                    currTime--;

                    document.getElementById("DisplayCountdown").innerHTML = currTime;
                    
                    currTime--;

                    document.getElementById("DisplayCountdown").innerHTML = currTime;
                    
                    currTime--;

                    document.getElementById("DisplayCountdown").innerHTML = currTime;
                    
                    currTime--;

                    // Print text for zero
                    document.getElementById("DisplayCountdown").innerHTML = "Blastoff!";
                </script>
